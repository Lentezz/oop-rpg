<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.Light.Distant?>
<?import javafx.scene.effect.Lighting?>
<?import javafx.scene.effect.Shadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="450.0" prefWidth="800.0" style="-fx-background-color: f4e55c;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ooprpg.controller.WinController">
   <children>
      <Button fx:id="buttonNewGame" layoutX="342.0" layoutY="275.0" mnemonicParsing="false" onAction="#newGameClicked" prefHeight="48.0" prefWidth="116.0" style="-fx-background-radius: 10;" text="New game">
         <effect>
            <Lighting diffuseConstant="1.78" surfaceScale="0.0">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant>
                     <color>
                        <Color red="0.9333333373069763" green="0.615686297416687" blue="0.3843137323856354" />
                     </color>
                  </Light.Distant>
               </light>
            </Lighting>
         </effect>
         <font>
            <Font name="Comic Sans MS Bold" size="15.0" />
         </font></Button>
      <Text layoutX="194.0" layoutY="212.0" strokeType="OUTSIDE" strokeWidth="0.0" text="YOU WIN!!!">
         <font>
            <Font name="Comic Sans MS Bold" size="72.0" />
         </font>
         <effect>
            <DropShadow height="38.585" radius="23.18875" width="56.17">
               <color>
                  <Color red="0.75" green="0.6734374761581421" blue="0.09375" />
               </color>
            </DropShadow>
         </effect></Text>
      <Button fx:id="buttonExit" layoutX="342.0" layoutY="354.0" mnemonicParsing="false" onAction="#exitClicked" prefHeight="48.0" prefWidth="116.0" style="-fx-background-radius: 10;" text="Exit">
         <effect>
            <Lighting diffuseConstant="1.78" surfaceScale="0.0">
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant>
                     <color>
                        <Color red="0.9342105388641357" green="0.6137964725494385" blue="0.3849293291568756" />
                     </color>
                  </Light.Distant>
               </light>
            </Lighting>
         </effect>
         <font>
            <Font name="Comic Sans MS Bold" size="15.0" />
         </font></Button>
   </children>
</AnchorPane>
